{
  "expo": {
    "name": "BAMS",
    "slug": "BAMS",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/yliam2.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/yliam2.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/yliam2.png"
    },
    "plugins": [
      "expo-barcode-scanner"
    ]
  }
}
